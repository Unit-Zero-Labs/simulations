{
    "version": 2,
    "builds": [
      {
        "src": "vercel-build.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "."
        }
      },
      {
        "src": "run.py",
        "use": "@vercel/python",
        "config": { "runtime": "python3.9" }
      }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "/static/$1" },
      { "src": "/(.*)", "dest": "/run.py" }
    ]
  }